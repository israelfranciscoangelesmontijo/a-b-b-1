#include <stdio.h>
main()
{
int a, b=0;
a=b*b+1;
printf("si b=%d, entonces a=%d\n", b, a);
b=2;
a=b*b+1;/*AÃ‘ADIR LA OPERACION EN LA NUEVA DEFINICION DE VARIABLES*/
printf("si b=%d, entonces a=%d\n", b, a);
b=-2;
a=b*b+1;
printf("si b=%d, entonces a=%d\n", b, a);	
}
